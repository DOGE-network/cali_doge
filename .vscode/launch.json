{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Process Department Spending",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npx",
      "runtimeArgs": ["tsx"],
      "args": ["${workspaceFolder}/src/scripts/process_department_spending.ts"],
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
} 